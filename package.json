{
  "name": "@url-shortener/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "generate-jwks": "ts-node scripts/generate-jwks.ts",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "prisma:generate": "prisma generate --schema=./libs/prisma-iam/src/prisma/schema.prisma && prisma generate --schema=./libs/prisma-url/src/prisma/schema.prisma",
    "prisma:reset": "prisma migrate reset --schema=./libs/prisma-iam/src/prisma/schema.prisma && prisma migrate reset --schema=./libs/prisma-url/src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./libs/prisma-iam/src/prisma/schema.prisma && prisma migrate dev --schema=./libs/prisma-url/src/prisma/schema.prisma",
    "tf:init": "cd terraform && terraform init",
    "tf:plan": "cd terraform && terraform plan",
    "tf:apply": "cd terraform && terraform apply",
    "tf:destroy": "cd terraform && terraform destroy",
    "kind:delete": "kind delete cluster --name url-shortener || true",
    "dev:setup": "npm run tf:init && npm run tf:plan && npm run tf:apply",
    "dev:clean": "npm run tf:destroy",
    "dev:rebuild": "npm run dev:clean && npm run dev:setup"
  },
  "private": true,
  "dependencies": {
    "@elastic/ecs-pino-format": "^1.5.0",
    "@nestjs/axios": "^3.1.2",
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/terminus": "^10.2.3",
    "@opentelemetry/exporter-jaeger": "^1.28.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.55.0",
    "@opentelemetry/instrumentation": "^0.55.0",
    "@opentelemetry/instrumentation-express": "^0.45.0",
    "@opentelemetry/instrumentation-http": "^0.55.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.42.0",
    "@opentelemetry/instrumentation-pino": "^0.44.0",
    "@opentelemetry/resources": "^1.28.0",
    "@opentelemetry/sdk-node": "^0.55.0",
    "@opentelemetry/sdk-trace-base": "^1.28.0",
    "@opentelemetry/sdk-trace-node": "^1.28.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "@prisma/instrumentation": "^5.22.0",
