    "@nx/workspace": "20.1.1",
    "@prisma/client": "^5.22.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.12",
